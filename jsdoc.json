{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
            "exclude":[
              "node_modules",
              "frontend",
              "docs",
              "routes",
              "assets",
              "jobqueue",
              "public",
              "pub_old",
              "views"
            ],
            "includePattern": ".+\\.js(doc|x)?$",
            "excludePattern": "(^|\\/|\\\\)_"
        },
    "sourceType": "module",
    "tags": {
            "allowUnknownTags": true,
            "dictionaries": ["jsdoc","closure"]
        },
    "templates": {
            "cleverLinks": false,
            "monospaceLinks": false
        }
}
